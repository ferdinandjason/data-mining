    "import os\n",
    "import cv2\n",
    "import math\n",
    "import pickle\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "ck_plus_data = []\n",
    "ck_plus_label = []\n",
    "clahe = cv2.createCLAHE(clipLimit=2.0, tileGridSize=(8,8))\n",
    "face_cascade = cv2.CascadeClassifier('./opencv/haarcascade_frontalface_default.xml')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_face(image_path):\n",
    "    image = cv2.imread(image_path, cv2.IMREAD_GRAYSCALE)\n",
    "    image_clahe = clahe.apply(image)\n",
    "    scale_factor = 1.25\n",
    "    faces = face_cascade.detectMultiScale(image_clahe, scale_factor, 5)\n",
    "    while not len(faces):\n",
    "        scale_factor -= 0.02\n",
    "        faces = face_cascade.detectMultiScale(image_clahe, scale_factor, 5)\n",
    "        if scale_factor <= 1:\n",
    "            print(image_path)\n",
    "            return\n",
    "    for (x,y,w,h) in faces:\n",
    "        face = cv2.resize(image_clahe[y:y+h, x:x+w], (128, 128), interpolation=cv2.INTER_AREA).flatten()\n",
    "        return face"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "def load_image_data(files_path, label):\n",
    "    global ck_plus_data\n",
    "    global ck_plus_label\n",
    "    threshold_neutral = math.floor((len(files_path)-1)*0.25)\n",
    "    threshold_labeled = math.floor((len(files_path)-1)*0.75)\n",
    "    for file in files_path:\n",
    "        if file.endswith('png'):\n",
    "            face_code = int(file[-12:-4])\n",
    "            if face_code <= threshold_neutral or face_code >= threshold_labeled :\n",
    "                face = get_face(file)\n",
    "                ck_plus_data.append(face)\n",
    "                # face = (face/max(face)).astype(np.float32)\n",
    "                if face_code <= threshold_neutral:\n",
    "                    ck_plus_label.append(0)\n",
    "                elif face_code >= threshold_labeled:\n",
    "                    ck_plus_label.append(label)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "def load_emotion_labels(emotion_label_path):\n",
    "    labels = dict()\n",
    "    for root, dirs, files in os.walk(emotion_label_path):\n",
    "        if dirs or not files:\n",
    "            continue\n",
    "        fid = os.sep.join(root.split(os.sep)[-2:])\n",
    "        for file in files:\n",
    "            f_name = os.path.join(root, file)\n",
    "            with open(f_name, 'r') as f:\n",
    "                labels[fid] = int(float(f.readline().strip()))\n",
    "    return labels"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "def load_extended_CK(image_path, emotion_label_path):\n",
    "    print('Emotion Label Processing...')\n",
    "    emotion_labels = load_emotion_labels(emotion_label_path)\n",
    "    print('Image Processing...')\n",
    "    for root, dirs, files in os.walk(image_path):\n",
    "        if dirs:\n",
    "            print(root)\n",
    "            continue\n",
    "        files_path = [os.path.join(root, file) for file in files]\n",
    "        fid = os.sep.join(root.split(os.sep)[-2:])\n",
    "        if fid in emotion_labels:\n",
    "            load_image_data(files_path, emotion_labels[fid])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [],
   "source": [
    "def pickle_extended_CK(image_path, emotion_label_path):\n",
    "    load_extended_CK(image_path, emotion_label_path)\n",
    "    \n",
    "    with open('./dataset/ck.plus.pickle', 'wb') as f:\n",
    "        global ck_plus_data\n",
    "        global ck_plus_label\n",
    "        print('Pickling dataset to \"./dataset/ck.plus.pickle\"')\n",
    "        database = [np.array(ck_plus_data), np.array(ck_plus_label)]\n",
    "        pickle.dump(database, f)"
   ]
  },
